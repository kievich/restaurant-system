<div class="modal-body">
    <div class="mb-3">
        <label for="formFile" class="form-label">Name</label>
        <input id="@ViewData.Eval("Prefix")-input-name" class="form-control" type="text" placeholder="" aria-label="">
    </div>

    <div class="mb-3">
        <label for="formFile" class="form-label">Table</label>
        <select id="@ViewData.Eval("Prefix")-input-tables" class="form-control" aria-label="Default select example">
            @foreach (var item in (IQueryable<Table>)ViewData.Eval("Tables"))
            {
                <option value="@item.Id">@item.Name</option>
            }
        </select>
    </div>
    <div class="mb-3">
        <label for="formFile" class="form-label">Start</label>
        <input id="@ViewData.Eval("Prefix")-input-start"  class="form-control" type="datetime-local" id="start" name="trip-start"
               value="2018-07-22">
    </div>
    <div class="mb-3">
        <label for="formFile" class="form-label">End</label>
        <input id="@ViewData.Eval("Prefix")-input-end"  class="form-control" type="datetime-local" id="start" name="trip-start"
               value="2018-07-22">
    </div>
</div>