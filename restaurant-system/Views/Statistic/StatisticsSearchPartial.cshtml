<div class="mb-3">
    <label for="formFile" class="form-label">Start Date</label>
    <input onfocusout="EditEventField()" id="statistic-start-date" value="@ViewData.Eval("StartDate")" class="form-control" type="datetime-local" name="trip-start">
</div>
<div class="mb-3">
    <label for="formFile" class="form-label">End Date</label>
    <input onfocusout="EditEventField()" id="statistic-end-date" value="@ViewData.Eval("EndDate")" class="form-control" type="datetime-local" name="trip-start">
</div>
<div class="mb-3">
    <button type="button" onclick="GetStatistic()" class="btn btn-primary">Search</button>
</div>

<script>
    function GetStatistic(){
         window.location.search = "?start=" + $("#statistic-start-date").val() + "&" + "end=" + $("#statistic-end-date").val();
    }

</script>